<?xml version="1.0"?>
<robot name="simple_box">

    <material name="blue">
        <color rgba= "0 0 1 1"/>
    </material>

    <material name="grey">
        <color rgba="0.75 0.75 0.75 1"/>
    </material>

    <!-- Turtlebot description -->
    <link name="base_link">
        <inertial>
            <mass value="1.0" />
            <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
        </inertial>

        <visual>
            <geometry>
                <box size="0.3 0.3 0.2" />
            </geometry>
        </visual>

        <collision>
            <geometry>
                <box size="0.3 0.3 0.2" />
            </geometry>
        </collision>
    </link>
</robot>